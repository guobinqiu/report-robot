{
  "name": "report-robot",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager ./ report-robot --out=./build --overwrite --platform=darwin",
    "package-win64": "electron-packager ./ report-robot --out=./build --overwrite --platform=win32 --arch=x64",
    "package-win32": "electron-packager ./ report-robot --out=./build --overwrite --platform=win32 --arch=ia32"
  },
  "repository": "https://github.com/guobinqiu/report-robot",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Github",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^10.1.0",
    "electron-packager": "^15.1.0"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "electron-store": "^6.0.0",
    "qs": "^6.9.4",
    "urlencode": "^1.1.0"
  }
}
